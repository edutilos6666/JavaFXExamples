<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.chart.ScatterChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edutilos.fxml.controller.ScatterChartExampleController">
   <children>
      <Label fx:id="lblTitle" layoutX="226.0" layoutY="22.0" text="ScatterChart Example">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <ScatterChart fx:id="chartScatter" layoutX="50.0" layoutY="47.0" prefHeight="299.0" prefWidth="500.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </ScatterChart>
      <Button fx:id="btnChange" layoutX="89.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleBtnChange" text="Change ScatterChart">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
