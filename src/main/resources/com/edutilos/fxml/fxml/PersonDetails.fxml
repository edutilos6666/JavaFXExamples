            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label fx:id="lblActive" layoutX="143.0" layoutY="272.0" text="Active">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="fieldActive" editable="false" layoutX="319.0" layoutY="267.0">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Button fx:id="btnClose" layoutX="438.0" layoutY="332.0" mnemonicParsing="false" onAction="#handleBtnClose" prefHeight="29.0" prefWidth="76.0" text="Close">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
