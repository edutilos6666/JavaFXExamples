         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="fieldId" editable="false" layoutX="319.0" layoutY="76.0">
         <font>
            <Font name="System Italic" size="15.0" />
         </font>
      </TextField>
      <Label fx:id="lblName" layoutX="143.0" layoutY="127.0" text="Name">
         <font>
            <Font name="System Bold" size="15.0" />
