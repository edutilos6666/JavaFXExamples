<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.edutilos.fxml.controller.PieChartExampleController">
   <children>
      <Label fx:id="lblTitle" layoutX="282.0" layoutY="27.0" text="PieChart Example">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
      <PieChart fx:id="chartPie" layoutX="50.0" layoutY="56.0" prefHeight="285.0" prefWidth="510.0" />
      <Button fx:id="btnChange" layoutX="70.0" layoutY="353.0" mnemonicParsing="false" onAction="#handleBtnChange" text="Change PieChart">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
